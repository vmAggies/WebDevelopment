<html>
  <head>

<% include head.ejs %>

	<style>
		<% include ../public/stylesheets/chart.css %>
	</style>
	

		
	<script type="text/javascript" src="http://www.amcharts.com/lib/3/amcharts.js"></script>
	<script type="text/javascript" src="http://www.amcharts.com/lib/3/serial.js"></script>
	<script type="text/javascript" src="http://www.amcharts.com/lib/3/themes/none.js"></script>
 		<script type="text/javascript">
   
		   var chart = AmCharts.makeChart("chartdiv", {
			    "theme": "none",
			    "type": "serial",
			    "dataProvider": [{
			        "Year": "2007",
			        "<%= output2[0]%>":  <%= output1[0]%>,
			        "<%= output2[1]%>": <%= output1[7]%>
			    }, {
			    	"Year": "2008",
			        "<%= output2[0]%>": <%= output1[1]%>,
			        "<%= output2[1]%>": <%= output1[8]%>
			    }, {
			    	"Year": "2009",
			        "<%= output2[0]%>": <%= output1[2]%>,
			        "<%= output2[1]%>": <%= output1[9]%>
			    }, {
			    	"Year": "2010",
			        "<%= output2[0]%>": <%= output1[3]%>,
			        "<%= output2[1]%>": <%= output1[10]%>
			    }, {
			    	"Year": "2011",
			        "<%= output2[0]%>": <%= output1[4]%>,
			        "<%= output2[1]%>": <%= output1[11]%>
			    }, {
			    	"Year": "2012",
			        "<%= output2[0]%>": <%= output1[5]%>,
			        "<%= output2[1]%>": <%= output1[12]%>
			    }, {
			    	"Year": "2013",
			        "<%= output2[0]%>": <%= output1[6]%>,
			        "<%= output2[1]%>": <%= output1[13]%>
			    }],
			    "valueAxes": [{
			    	"title": "Funds in Dollars"
			    }],
			    "startDuration": 1,
			    "graphs": [{
			        "balloonText": "Funds given in [[category]] to <%= output2[0]%>: <b>[[value]]</b>",
			        "fillAlphas": 0.9,
			        "lineAlpha": 0.2,
			        "title": "<%= output2[0]%>",
			        "type": "column",
			        "valueField": "<%= output2[0]%>"
			    }, {
			        "balloonText": "Funds given in [[category]] to <%= output2[1]%>: <b>[[value]]</b>",
			        "fillAlphas": 0.9,
			        "lineAlpha": 0.2,
			        "title": "<%= output2[1]%>",
			        "type": "column",
			        "valueField": "<%= output2[1]%>"
			    }],
			    "plotAreaFillAlphas": 0.1,
			    "depth3D": 60,
			    "angle": 30,
			    "categoryField": "Year",
			    "categoryAxis": {
			        "gridPosition": "start"
			    },
				"exportConfig":{
					"menuTop":"20px",
			        "menuRight":"20px",
			        "menuItems": [{
			        "icon": '/lib/3/images/export.png',
			        "format": 'png'	  
			        }]  
			    }
			});
		    
		  </script>
  </head>
  
  <body id="page3" class="pageChart">
	<!-- header -->
	<div class="bg">
		<div class="main">
			 <% include header.ejs %>
			 <section id="content">
		    	<p class="chartdesc" align="center">Comparison between <%= output2[0]%> and <%= output2[1]%></p>
		    	<div id="chartdiv" ><img src="loading.gif" /></div>
		    	<% include midder.ejs %> 
		    </section>
		</div>
		<% include footer.ejs %>
	</div>
  </body>

</html>